{
  "name": "unplugin-no-custom-resolving",
  "type": "module",
  "version": "0.0.0",
  "description": "An optionated unplugin plugin to suppress for JS / TS custom resolving",
  "license": "MIT",
  "homepage": "https://github.com/kazupon/no-custom-resolving/packages/unplugin-no-custom-resolving#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/no-custom-resolving.git",
    "directory": "packages/unplugin-no-custom-resolving"
  },
  "bugs": {
    "url": "https://github.com/kazupon/no-custom-resolving/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch src",
    "build": "tsup",
    "build:fix": "esno scripts/postbuild.ts",
    "play": "npm -C playground run dev",
    "play:build": "npm -C playground run build",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "webpack": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    }
  },
  "dependencies": {
    "unplugin": "^1.4.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.10.2",
    "@nuxt/schema": "^3.10.2",
    "chalk": "^5.3.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "rollup": "^4.12.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.2",
    "vitest": "^1.3.0",
    "webpack": "^5.88.2"
  }
}
